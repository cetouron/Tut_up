 </v-btn>\n\n        <v-btn class=\"couleurPrimaire\" v-if=\"this.pfl=='Eleve'\">\n          <v-icon>mdi-star</v-icon>\n          Premium\n        </v-btn>\n\n<v-btn  v-if=\"this.pfl=='pasco'\" to='connexion'>\n          <v-icon>mdi-account</v-icon>\n          Connexion\n        </v-btn>\n\n        <v-btn  v-if=\"this.pfl=='pasco'\" to='inscription'>\n          <v-icon>mdi-account</v-icon>\n          Inscription\n        </v-btn>\n\n\n        <!--     <v-btn @click=\"show = !show\" text>\n          <v-icon>mdi-message</v-icon>\n          Message\n        </v-btn> -->\n\n        <!--\n        <v-btn to=\"connexion\" text>\n          <v-icon>mdi-account</v-icon>\n          Connexion\n        </v-btn> -->\n      </v-toolbar-items>\n    </v-app-bar>\n\n<!--    <v-navigation-drawer v-model=\"drawer\" absolute temporary right>\n      <v-list dense>\n        <v-list-item\n          v-for=\"item in items\"\n          :key=\"item.title\"\n          :to=\"item.link\"\n          link\n        >\n          <v-list-item-icon>\n            <v-icon>mdi-{{ item.icon }}</v-icon>\n          </v-list-item-icon>\n\n          <v-list-item-content>\n            <v-list-item-title>{{ item.title }}</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer> --> \n  </div>\n</template>\n\n<script>\n\nimport { mapState, mapMutations } from \"vuex\";\n\n\nexport default {\n  components: {\n  \n  },\n    computed: {\n    ...mapState([\"pfl\"]),\n  },\n  methods: {\n    mssg: function () {\n      this.showP = false;\n      this.showPT = false;\n      this.show = !this.show;\n    },\n    prfl: function () {\n      this.show = false;\n      this.showPT = false;\n      this.showP = !this.showP;\n    },\n    prflT: function () {\n      this.show = false;\n      this.showP = false;\n      this.showPT = !this.showPT;\n    },\n  },\n  data() {\n    return {\n      drawer: false,\n      show: false,\n      showP: false,\n      showPT: false,\n      items: [\n        { title: \"Home\", link: \"/\", icon: \"home\" },\n        { title: \"Store\", link: \"store\", icon: \"store\" },\n        { title: \"Cart\", link: \"cart\", icon: \"cart\" },\n        { title: \"Recherche\", link: \"recherche\", icon: \"feature-search\" },\n        { title: \"Mesagerie\", link: \"chat\", icon: \"message\" },\n\n      ],\n    };\n  },\n};\n</script>\n"]}]}